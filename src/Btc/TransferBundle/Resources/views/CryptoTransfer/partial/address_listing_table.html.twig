{% trans_default_domain 'Deposit' %}
<table class="table table-bordered table-hover table-striped">
    <thead>
    <tr>
        <th>{{ 'coin.table.date'|trans }}</th>
        <th>{{ 'coin.table.address'|trans }}</th>
    </tr>
    </thead>
    <tbody>
    {% for addr in addresses %}
        <tr>
            <td>{{ addr.createdAt|date('Y-m-d H:i:s') }}</td>
            <td>{{ addr.address }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
